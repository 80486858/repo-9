            </expect>

            <expect>
                8=FIX\.4\.1#9=\d{1,3}#35=D#34=6#43=Y#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}#56=TW#122=\d{8}-\d{2}:\d{2}:\d{2}#11=ID#21=3#40=1#54=1#55=CDG#38=1#10=\d{3}#
            </expect>

            <!--# Admin messages 2=4, App messages 5=6, Admin messages 7=8-->

            <send>8=FIX.4.1#9=123#35=2#34=12#49=TW#52=20031027-14:29:11#56=ISLD#7=2#16=8#10=000#</send>
            <expect>
                8=FIX\.4\.1#9=\d{1,3}#35=4#34=2#43=Y#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}#56=TW#122=\d{8}-\d{2}:\d{2}:\d{2}#36=5#123=Y#10=\d{3}#

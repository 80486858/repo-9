            <!--# logout message and response-->

            <send>8=FIX.4.1#9=123#35=5#34=16#49=TW#52=20031027-14:29:11#56=ISLD#10=000#</send>
            <expect>8=FIX\.4\.1#9=\d{1,3}#35=5#34=11#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}#56=TW#10=\d{3}#</expect>

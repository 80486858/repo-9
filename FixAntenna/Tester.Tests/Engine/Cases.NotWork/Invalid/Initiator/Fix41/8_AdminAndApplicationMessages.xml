            </expect>

            <!--# Application Messages 5=6-->

            <send>
                8=FIX.4.1#9=123#35=D#34=5#49=TW#52=20031027-14:29:11#56=ISLD#11=ID#21=3#40=1#54=1#38=4#55=INTC#10=000#
            </send>
            <expect>
                8=FIX\.4\.1#9=\d{1,3}#35=D#34=5#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}#56=TW#11=ID#21=3#40=1#54=1#38=4#55=INTC#10=\d{3}#

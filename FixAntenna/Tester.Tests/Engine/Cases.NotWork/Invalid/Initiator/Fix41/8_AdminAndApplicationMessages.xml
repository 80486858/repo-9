            <expect>
                8=FIX\.4\.1#9=\d{1,3}#35=D#34=9#43=Y#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}#56=TW#122=\d{8}-\d{2}:\d{2}:\d{2}#11=ID#21=3#40=1#54=1#55=IVP#38=1#10=\d{3}#
            </expect>

            <!--# make sure our seq numbers are still correct-->

            <send>8=FIX.4.1#9=123#35=1#34=15#49=TW#52=20031027-14:29:11#56=ISLD#112=HELLO#10=000#</send>
            <expect>8=FIX\.4\.1#9=\d{1,3}#35=0#34=10#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}#56=TW#112=HELLO#10=\d{3}#

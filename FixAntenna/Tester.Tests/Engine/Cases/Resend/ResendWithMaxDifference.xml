            <send>8=FIX.4.4#9=0#35=6#34=2#49=TW#52=20031027-14:29:11#56=ISLD#23=2#28=N#27=1#55=1#54=1#10=000#</send>
            <expect>
                8=FIX\.4\.4#9=\d{1,3}#35=6#34=2#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}(\.\d{3})?#56=TW#10=\d{3}#

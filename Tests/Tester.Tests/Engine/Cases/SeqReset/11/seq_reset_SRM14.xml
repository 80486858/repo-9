<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE cases SYSTEM "../../../../cases.dtd" [
        <!ENTITY % content SYSTEM "../../../../Engine/Entities/definitions.inc">
        %content; ]>

<cases>

    <!--case SRM14 -->
    <!--testzilla case 11a_002_SR -->
    <!--Step 1. Receive Sequence Reset (Reset) message with NewSeqNo > than expected sequence number
                and MsgSeqNumber > than expected sequence number  -->
    <!--Step 2. 8=FIX.4.4 9=53 35=4 49=SNDR 56=TRGT 34=5 52=20081225-18:32:15 36=10 10=150 -->
    <!--Step 3. Sequence Reset message should be accepted without
                regards to its MsgSeqNum and expected SeqNum should be changed to 10 -->

    <case name="SRM14">
        &CLEAN_LOGS;
        &ENGINE_UP;
        <transport className="InitiatorSocketTransport" host="localhost" port="3000">
            <send>8=FIX.4.4#9=123#35=A#34=1#49=TW#52=20031027-14:29:11#56=ISLD#98=0#108=5#10=000#</send>
            <expect>
                 8=FIX\.4\.4#9=\d{1,3}#35=A#34=1#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}(\.\d{3})?#56=TW#10=\d{3}#
            </expect>

            <send>8=FIX.4.4#9=0#35=4#34=5#49=TW#52=20031027-14:29:11#56=ISLD#36=10#10=000#</send>
            <expect>
                8=FIX\.4\.4#9=\d{1,3}#35=0#34=2#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}(\.\d{3})?#56=TW#10=\d{3}#
            </expect>

            <send>8=FIX.4.4#9=0#35=0#34=10#49=TW#52=20031027-14:29:11#56=ISLD#10=000#</send>
            <expect>
                8=FIX\.4\.4#9=\d{1,3}#35=0#34=3#49=ISLD#52=\d{8}-\d{2}:\d{2}:\d{2}(\.\d{3})?#56=TW#10=\d{3}#
            </expect>
        </transport>
        &ENGINE_DOWN;
    </case>
</cases>
